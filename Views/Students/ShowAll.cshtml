@model List<Students>
<table class="table table-hover table-bordered">
    <tr>
        <th>Number</th>
        <th>Name</th>
        <th>Age</th>
        <th>Departmentid</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Age</td>
            <td>@item.Departmentid</td>
        </tr>
    }
</table>